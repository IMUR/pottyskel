{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"target": "ES2019",
		"module": "ESNext",
		"moduleResolution": "node",
		"lib": ["ES2019", "DOM"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"types": ["svelte", "node", "@types/express"]
	},
	"include": ["src/**/*.ts", "src/**/*.svelte"],
	"exclude": ["node_modules"]
}
